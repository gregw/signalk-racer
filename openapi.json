  "paths": {
    "/startline/port": {
      "get": {
        "summary": "Set the port end of the start line",
        "responses": {
          "200": { "description": "Port end set" }
        }
      }
    },
    "/startline/stb": {
      "put": {
        "summary": "Set the starboard end of the start line",
        "description": "Set the starboard end of the start line to the boats position plus the bow offset.",
        "responses": {
          "200": { "description": "Starboard end set" }
        }
      }
    }
  }
}